{
  "name": "aws-lambda",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.11.15",
    "esbuild": "^0.16.7",
    "prisma": "^4.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.234.0",
    "@fastify/aws-lambda": "^3.1.3",
    "@prisma/client": "^4.7.1",
    "fastify": "^4.10.2"
  },
  "scripts": {
    "start": "ts-node src/app.ts",
    "build": "yarn prebuild && esbuild src/lambda.ts --bundle --minify --sourcemap --platform=node --target=es2020 --outfile=dist/index.js",
    "prebuild": "rm -rf dist",
    "postbuild": "cd dist && zip -r index.zip index.js*"
  }
}
